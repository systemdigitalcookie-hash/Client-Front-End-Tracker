{
  "version": 2,
  "routes": [
    {
      "src": "^/t/[^/]+$",
      "dest": "/index.html"
    },
    {
      "src": "^/api/project/([^/]+)$",
      "dest": "/api/project/[id].js?id=$1"
    },
    {
      "src": "^/script\\.js$",
      "dest": "/script.js"
    },
    {
      "src": "^/style\\.css$",
      "dest": "/style.css"
    },
    {
      "src": "^/$",
      "dest": "/index.html"
    }
  ]
}